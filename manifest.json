{
  "name": "AutoScroll",
  "version": "0.0.2",
  "description": "Autoscrolling for the lazy!",
  "permissions": ["activeTab", "scripting", "declarativeContent", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/scroll.png"
    }
  },
  "icons": {
    "128": "images/scroll.png",
    "48": "images/scroll copy.png"
  },
  "manifest_version": 3
}